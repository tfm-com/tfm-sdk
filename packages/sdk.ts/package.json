{
  "name": "@0xtfm/sdk",
  "version": "0.2.5",
  "description": "TFM SDK",
  "author": "TFM",
  "homepage": "https://github.com/tfm-com/tfm-sdk.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tfm-com/tfm-sdk.ts.git"
  },
  "license": "MIT",
  "keywords": [
    "tfm",
    "sdk",
    "bridge",
    "swap",
    "route",
    "ibc"
  ],
  "main": "dist/cjs/@0xtfm/sdk.js",
  "module": "dist/esm/@0xtfm/sdk.js",
  "browser": "dist/browser/@0xtfm/sdk.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rollup -c",
    "prebuild:types": "rimraf ./build",
    "build:types": "tsc -p ./tsconfig.json --outDir build --declaration true && api-extractor run",
    "predocs": "rimraf ./docs",
    "docs": "typedoc",
    "clean": "rimraf ./build ./dist ./docs",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "test": "jest"
  },
  "dependencies": {
    "apisauce": "^3.0.1",
    "json-with-bigint": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.8",
    "@babel/preset-env": "^7.22.7",
    "@babel/preset-typescript": "^7.22.5",
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@microsoft/api-extractor": "^7.19.4",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/jest": "^29.5.2",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.6.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.66.1",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.4.0",
    "tslib": "^2.3.1",
    "typedoc": "^0.22.11",
    "typescript": "^4.5.5"
  },
  "bugs": {
    "url": "https://github.com/tfm-com/tfm-sdk.ts/issues"
  },
  "directories": {
    "doc": "docs"
  }
}
